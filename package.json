{
  "name": "system_enr",
  "version": "0.1.0",
  "main": "http://localhost:8080",
  "node-remote": "http://localhost:8080",
  "license": "",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "nw-serve": "nw .",
    "nw-build": "build --tasks linux-x64 --mirror https://dl.nwjs.io/ ."
  },
  "dependencies": {
    "axios": "^0.18.0",
    "core-js": "2.6.5",
    "js-cookie": "^2.2.0",
    "nw-vue-devtools-prebuilt": "^0.0.10",
    "nwjs-builder-phoenix": "^1.15.0",
    "qs": "^6.9.4",
    "vue": "^2.5.17",
    "vue-html-to-paper": "^1.3.1",
    "vue-router": "^3.0.1",
    "vue-web-cam": "^1.9.0",
    "vuejs-datepicker": "^1.6.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.12",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.12",
    "@vue/cli-service": "^3.0.0-rc.12",
    "nw": "^0.50.2-sdk",
    "vue-awesome": "^4.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "build": {
    "nwVersion": "0.49.2",
    "strippedProperties": [
      "chromium-args"
    ],
    "targets": [
      "zip",
      "nsis7z"
    ],
    "excludes": [
      "node_modules/*",
      "src/**/*",
      "public/**/*",
      ".eslintrc",
      ".gitignore",
      "yarn.lock",
      "package-lock.json",
      "vue.config.js"
    ]
  },
  "window": {
    "title": "system_enr",
    "toolbar": true,
    "frame": false,
    "fullscreen": false,
    "width": 300,
    "height": 300,
    "position": "center",
    "icon": "/public/static/favicon-32.ico",
    "print": {
      "autoprint": true,
      "silent": false,
      "headerFooterEnabled": false
    }
  },
  "webkit": {
    "plugin": true
  },
  "developer": {
    "showDevToolsOnStartup": true
  },
  "chromium-args": "--password-store=basic --load-extension='./node_modules/nw-vue-devtools-prebuilt/extension' --remote-debugging-port=9222",
  "manifestUrl": "http://127.0.0.1:3127/package.json"
}
